<!DOCTYPE html>
<html>
  <head>
    <meta charset='utf-8'>
    <title>Scrabble Score</title>
    <link rel='stylesheet' href='https://maxcdn.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css'>
    <link rel='stylesheet' href='~/css/styles.css'>
  </head>
  <body>
    <div class="container">
      <div class="title">
        @foreach(var c in "Scrabble_Score")
        {
          <span class="title-tile">@c</span>
        }
      </div>
      <br>
        @RenderBody()
    </div>
  </body>
</html>
